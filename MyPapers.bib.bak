@Article{Gaens2013kinetic,
  author =   {W Van Gaens and A Bogaerts},
  title =    {Kinetic modelling for an atmospheric pressure argon plasma jet in humid air},
  journal =  {Journal of Physics D: Applied Physics},
  year =     {2013},
  volume =   {46},
  number =   {27},
  pages =    {275201},
  abstract = {A zero-dimensional, semi-empirical model is used to describe the plasma chemistry in an argon plasma jet flowing into humid air, mimicking the experimental conditions of a setup from the Eindhoven University of Technology. The model provides species density profiles as a function of the position in the plasma jet device and effluent. A reaction chemistry set for an argon/humid air mixture is developed, which considers 84 different species and 1880 reactions. Additionally, we present a reduced chemistry set, useful for higher level computational models. Calculated species density profiles along the plasma jet are shown and the chemical pathways are explained in detail. It is demonstrated that chemically reactive H, N, O and OH radicals are formed in large quantities after the nozzle exit and H 2 , O 2 ( 1 Î g ), O 3 , H 2 O 2 , NO 2 , N 2 O, HNO 2 and HNO 3 are predominantly formed as âlong livingâ species. The simulations show that water clustering of positive ions is very important under these conditions. The influence of vibrational excitation on the calculated electron temperature is studied. Finally, the effect of varying gas temperature, flow speed, power density and air humidity on the chemistry is investigated.},
  file =     {:/Users/hld523/Google Drive/Plasmaaaaaaaaaaa/Papers/Modelling/Gaens2013 Kinetic modelling for an atmospheric pressure argon plasma jet in humid air.pdf:PDF},
  url =      {http://stacks.iop.org/0022-3727/46/i=27/a=275201}
}

