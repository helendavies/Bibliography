@Article{Kim2014,
  author =   {Kim, Yong Hee and Hong, Young June and Baik, Ku Youn and Kwon, Gi Chung and Choi, Jin Joo and Cho, Guang Sup and Uhm, Han Sup and Kim, Do Young and Choi, Eun Ha},
  title =    {Measurement of Reactive Hydroxyl Radical Species Inside the Biosolutions During Non-thermal Atmospheric Pressure Plasma Jet Bombardment onto the Solution},
  journal =  {Plasma Chemistry and Plasma Processing},
  year =     {2014},
  volume =   {34},
  number =   {3},
  pages =    {457--472},
  abstract = {Non-thermal atmospheric pressure plasma jet could generate various kinds of radicals on biosolution surfaces as well as inside the biosolutions. The electron temperature and ion density for this non-thermal plasma jet have been measured to be about 0.8{\textasciitilde}1.0 eV and 1 {\texttimes} 1013 cm−3 in this experiment, respectively, by atmospheric pressure collisional radiative model and ion collector current. In this context, the hydroxyl OH radical density inside the biosolutions has been also investigated experimentally by ultraviolet absorption spectroscopy when the Ar non-thermal plasma jet has been bombarded onto them. It is found that the emission and absorption profiles for the other reactive oxygen species such as NO (226 nm) and O2*− (245 nm) are shown to be very small inside the biosolution in comparison with those for the OH radical species. It is found that the densities of OH radical species inside the biosolutions are higher than those on the surface in this experiment. The densities of the OH radical species inside the deionized water, Dulbecco's modified eagle medium, and phosphate buffered saline are measured to be about 2.1 {\texttimes} 1016, 1.1 {\texttimes} 1016, and 1.0 {\texttimes} 1016 cm−3, respectively, at 2 mm downstream from the surface under optimized Ar gas flow of 200 sccm. It is also found that the critical hydroxyl OH radical density for the lung cancer H460 cells to experience an apoptosis is observed to be around 0.3 {\texttimes} 1016 cm−3 under 1 min plasma exposure in this experiment.},
  doi =      {10.1007/s11090-014-9538-0},
  issn =     {1572-8986},
  url =      {http://dx.doi.org/10.1007/s11090-014-9538-0}
}

