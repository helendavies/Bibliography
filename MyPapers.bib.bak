@Article{Sies2017oxidative,
  author =   { Helmut Sies and Carsten Berndt and Dean P. Jones},
  title =    {Oxidative Stress},
  journal =  {Annual Review of Biochemistry},
  year =     {2017},
  volume =   {86},
  number =   {1},
  pages =    {null},
  note =     {PMID: 28441057},
  abstract = { Oxidative stress is two sided: Whereas excessive oxidant challenge causes damage to biomolecules, maintenance of a physiological level of oxidant challenge, termed oxidative eustress, is essential for governing life processes through redox signaling. Recent interest has focused on the intricate ways by which redox signaling integrates these converse properties. Redox balance is maintained by prevention, interception, and repair, and concomitantly the regulatory potential of molecular thiol-driven master switches such as Nrf2/Keap1 or NF-ÎºB/IÎºB is used for system-wide oxidative stress response. Nonradical species such as hydrogen peroxide (H2O2) or singlet molecular oxygen, rather than free-radical species, perform major second messenger functions. Chemokine-controlled NADPH oxidases and metabolically controlled mitochondrial sources of H2O2 as well as glutathione- and thioredoxin-related pathways, with powerful enzymatic back-up systems, are responsible for fine-tuning physiological redox signaling. This makes for a rich research field spanning from biochemistry and cell biology into nutritional sciences, environmental medicine, and molecular knowledge-based redox medicine. Expected final online publication date for the Annual Review of Biochemistry Volume 86 is June 20, 2017. Please see http://www.annualreviews.org/page/journal/pubdates for revised estimates. },
  comment =  {Comprehensive review of oxidative stress, but maybe more focussed on eukaryotic rather than prokaryotic stuff. Suggests lots of different markers/products of oxidative stress, including ones I've already heard about which is promising!},
