@Article{Ayan2009application,
  author =   {H Ayan and D Staack and G Fridman and A Gutsol and Y Mukhin and A Starikovskii and A Fridman and G Friedman},
  title =    {Application of nanosecond-pulsed dielectric barrier discharge for biomedical treatment of topographically non-uniform surfaces},
  journal =  {Journal of Physics D: Applied Physics},
  year =     {2009},
  volume =   {42},
  number =   {12},
  pages =    {125202},
  abstract = {Antimicrobial effectiveness of a nanosecond-pulsed dielectric barrier discharge (DBD) was investigated and compared with that of a microsecond-pulsed DBD. Experiments were conducted on the Escherichia coli bacteria covering a topographically non-uniform agar surface acting as one of the DBD electrodes. They reveal that the nanosecond-pulsed DBD can inactivate bacteria in recessed areas whereas the microsecond-pulsed and conventional DBDs fail to do so. Charged species (electrons and ions) appear to play the major role in the bacteria inactivation with the nanosecond-pulsed DBD. Moreover, the nanosecond-pulsed DBD kills bacteria significantly faster than its microsecond-pulsed counterpart.},
  comment =  {Paper comparing nanosecond and microsecond pulsed DBDs using patterned (not flat) agar. Nanosecond was shown to be more uniform and ignited evenly over the whole agar plate, whereas microsecond plasma was filamentary and only ignited at points of the agar. E. Coli was used and nanosecond plasma was found to be more efficient at killing E. Coli, including on patterned agar. With an air flow parallel to the agar surface, the killing area was not increased suggesting that it is species that are in the plasma in direct contact with the bacteria that mediate the killing, including charged particles. Interesting bit about the importance of rise time and why it needs to be fast for the discharge to be uniform.},
  file =     {:/Users/hld523/Google Drive/Plasmaaaaaaaaaaa/Papers/Ayan2009 Application of nanosecond-pulsed dielectric barrier discharge for biomedical treatment of topographically non-uniform surfaces.pdf:PDF},
  keywords = {Nanosecond DBD, microsecond DBD, E. Coli, Uneven electrodes, air plasma},
  url =      {http://stacks.iop.org/0022-3727/42/i=12/a=125202}
}

